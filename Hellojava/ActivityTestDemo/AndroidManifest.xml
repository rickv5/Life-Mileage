<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qianshang.activitytestdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 梁万古的activity -->
        <activity android:name="com.qianshang.activitytestdemo.liangwangu"
           android:screenOrientation="portrait" ></activity>
        <!-- 梁成的activity -->
        <activity android:name="com.qianshang.activitytestdemo.liangcheng"
            android:screenOrientation="portrait"></activity>
        <!-- 梁谊的activity -->
        <activity android:name="com.qianshang.activitytestdemo.liangyi"
            android:screenOrientation="portrait"></activity>
        <!-- 梁振轩的activity -->
        <activity android:name="com.qianshang.activitytestdemo.liangzhenxuan"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
